[run]
source = src
omit = 
    */tests/*
    */__pycache__/*
    */venv/*
    */.venv/*
    */site-packages/*
    src/main.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    Protocol
    
precision = 2
show_missing = True

[html]
directory = htmlcov
